llback.\n      if (this.onClose !== null) {\n        this.onClose()\n      }\n    },\n\n    open() {\n      // First, call a defined callback.\n      if (this.isDef(this.onOpen)) {\n        this.onOpen()\n      }\n\n      this.modalInstance.show();\n    },\n\n    isDef(obj) {\n      return typeof obj !== undefined && obj !== null;\n    },\n  }\n}\n</script>\n"],"sourceRoot":"node_modules/vue-bootstrap4-modal/src"}]}